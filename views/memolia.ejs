    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        
        body {
            height: 100vh;
            width: 100vw;
            font-family: sans-serif;
            overflow: hidden;
        }
        
        .scene, canvas {
            position: absolute;
            height: 100%;
            width: 100%;
        }
        #mobileInterface {
            position: absolute;
            width: 100%;
            height: 120px;

            
            z-index: 11;
            top: calc(100% - 120px);
            left: 0;
            display: none;
            
        }
        #joyWrapper {
            
            touch-action: manipulation;
            pointer-events: none;
        }
        #joystickWrapper1 {
            pointer-events: auto;
            display: block;
            position: absolute;
            left: calc(50% - 60px);
            background-color: transparent;
           
            transform: translateY(-50%);

            z-index: 12;
            touch-action: manipulation;
        }
        #buttonWrapper {
            height: 75px;
            width: 100px;
            position: absolute;
            top: 50%;
            left: calc(50% + 70px);
            transform: translateY(-50%);
        }
        
        #lookDir {
            height: 100%;
            width: 66%;
            top: 0;
            left: 0;
            position: absolute;
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: 12px;
        }
        #up, #down {
            width: 100%;
            height: 100%;
            max-height: 50px;
            padding-top: 10px;
            margin: 1px;
            box-sizing: border-box;
            background: white;
            border-radius: 5px;
        }
        #JumpB {
            height: 100%;
            width: 33%;
            top: 0;
            right: 0;
            position: absolute;
            background: #dbdbdb;
            clip-path: polygon(20% 10%, 80% 10%, 100% 20%, 100% 80%, 80% 90%, 20% 90%, 0% 80%, 0 20%);

        }
        .noSelect {
            user-select: none;
        }
    </style>
</head>
